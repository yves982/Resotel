            <sys:DateTime x:Key="endDate">2016-07-03</sys:DateTime>
            <DataTemplate DataType="{x:Type rvme:OptionChoiceEntity}">
                <rvc:SumUpOptionView></rvc:SumUpOptionView>
            </DataTemplate>
        </Grid.Resources>
